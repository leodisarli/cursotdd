{
    "name": "leodisarli/disarl-cursotdd",
    "description": "curso tdd",
    "keywords": ["framework", "laravel", "lumen", "tdd"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Leo Di Sarli",
            "email": "leosarli@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "optimize-autoloader": true
}